{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "AllowedHosts": "*",
    "JwtSecurityKey": "RANDOM_KEY_MUST_NOT_BE_SHARED",
    "JwtIssuer": "http://localhost:52/AuthJWT",
    "JwtAudience": "http://localhost:52/Messages",
    "JwtExpiryInDays": 1,
    "JwtExpiryInMinutes": 30
  }
}
